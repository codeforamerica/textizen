<div class="nested-fields">
  <fieldset>
  	<%= f.input :value %>
  	<%= f.input :text %>
    <%= link_to_remove_association "remove followup option", f %>
    <%= f.simple_fields_for :follow_up do |followup| %>
      <%= render 'question_fields', :f => followup %>  
    <% end %>
  </fieldset>
</div>
